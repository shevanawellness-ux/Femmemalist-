// ==================== BATCH RENDER ====================
const container = document.getElementById('videoContainer');
const loadBtn = document.getElementById('loadMoreBtn');
let currentIndex = 0;
const batchSize = 30;

function loadVideos() {
  const end = Math.min(currentIndex + batchSize, videoIDs.length);
  for(let i=currentIndex; i<end; i++){
    const divTitle = document.createElement('div');
    divTitle.className = 'video-title';
    divTitle.textContent = `Video ${i+1}`;
    container.appendChild(divTitle);

    const divGrid = document.createElement('div');
    divGrid.className = 'video-grid yt-lite';
    divGrid.dataset.id = videoIDs[i];

    const img = document.createElement('img');
    img.src = `https://i.ytimg.com/vi/${videoIDs[i]}/hqdefault.jpg`;
    img.style.objectFit='cover';
    img.style.width='100%';
    img.style.height='100%';
    divGrid.appendChild(img);

    divGrid.onclick = () => {
      divGrid.innerHTML = `<iframe loading="lazy" src="https://www.youtube.com/embed/${videoIDs[i]}?autoplay=1" frameborder="0" allowfullscreen style="width:100%;height:100%;border-radius:14px;"></iframe>`;
    };
    container.appendChild(divGrid);
  }
  currentIndex = end;
  if(currentIndex >= videoIDs.length){
    loadBtn.style.display = 'none';
  }
}

// Load initial 100 videos
currentIndex = 0;
for(let i=0; i<100 && i<videoIDs.length; i+=batchSize){
  loadVideos();
}

// Load more button
loadBtn.addEventListener('click', loadVideos);
</script>

